using System;
using System.Runtime.Serialization;

namespace Com.Suncor.Olt.Common.Exceptions
{
    [Serializable]
    public class ExpectedUnitLevelFLOCException : OLTException
    {
        public ExpectedUnitLevelFLOCException()
            : base("Expecting Unit Level FLOC.")
        {
        }

        public ExpectedUnitLevelFLOCException(string message)
            : base(message)
        {
        }

        public ExpectedUnitLevelFLOCException(string message, Exception innerException)
            : base(message, innerException)
        {
        }

        public ExpectedUnitLevelFLOCException(SerializationInfo info, StreamingContext context)
            : base(info, context)
        {
        }
    }
}