namespace Com.Suncor.Olt.Common.Domain
{
    public class LogTemplateWorkAssignment : DomainObject
    {
        private readonly long logTemplateId;
        private readonly WorkAssignment workAssignment;

        public LogTemplateWorkAssignment(long logTemplateId, WorkAssignment workAssignment)
        {
            this.logTemplateId = logTemplateId;
            this.workAssignment = workAssignment;
        }

        public long LogTemplateId
        {
            get { return logTemplateId; }
        }

        public WorkAssignment WorkAssignment
        {
            get { return workAssignment; }
        }
    }
}