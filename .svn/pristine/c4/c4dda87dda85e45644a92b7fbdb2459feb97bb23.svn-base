using System;
using System.Runtime.Serialization;

namespace Com.Suncor.Olt.Common.Exceptions
{
    [Serializable]
    public class ExpectedNonDivisionLevelFLOCException : OLTException
    {
        public ExpectedNonDivisionLevelFLOCException() : base("Expecting FLOC that is not Level 1 (Division).")
        {
        }

        public ExpectedNonDivisionLevelFLOCException(string message) : base(message)
        {
        }

        public ExpectedNonDivisionLevelFLOCException(string message, Exception innerException)
            : base(message, innerException)
        {
        }

        public ExpectedNonDivisionLevelFLOCException(SerializationInfo info, StreamingContext context)
            : base(info, context)
        {
        }
    }
}