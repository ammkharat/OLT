update commentcategory
set LogGuidelines = 
'- Change in equipment status' + CHAR(13) + CHAR(10) +
'        - Example: LOTO, pump switches, failures (seal, instrumentation, etc.), PMs, etc' + CHAR(13) + CHAR(10) +
'' + CHAR(13) + CHAR(10) +
'- Variations form targets, KMPs and rundown sheets' + CHAR(13) + CHAR(10) +
'        - Include reason for variations' + CHAR(13) + CHAR(10) +
'' + CHAR(13) + CHAR(10) +
'- Identify any operational difficulties (e.g., tower flooding)' + CHAR(13) + CHAR(10) +
'        - Include a summary of troubleshooting efforts undertaken' + CHAR(13) + CHAR(10) +
'' + CHAR(13) + CHAR(10) +
'- # of technician short on shift'
where Id =
(
	select CommentCategoryId
	from CommentCategoryFunctionalLocation
	where FunctionalLocationId =
	(
		select f.Id
		from FunctionalLocation f
		where f.SiteId = 8
		and Level = 1
	)
)

