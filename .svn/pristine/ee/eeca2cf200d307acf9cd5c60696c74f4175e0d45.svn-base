-- Child Level 2, Parent Level 1
INSERT INTO FunctionalLocationAncestor ([Id], [AncestorId], [AncestorLevel]) (
select c.id, a.id, a.[Level] from FunctionalLocation c,
functionallocation a
where 
a.siteid = c.siteid
and
c.[level] = 2
and
c.[level] > a.[level]
and 
c.Division = a.Division
and NOT EXISTS(select id from FunctionalLocationAncestor where id = c.id and ancestorid = a.id)
)
