using System;
using System.Runtime.Serialization;

namespace Com.Suncor.Olt.Common.Exceptions
{
    [Serializable]
    public class SqlWrapperException : OLTException
    {
        public SqlWrapperException(string message, Exception innerException) : base(message, innerException)
        {
        }


        public SqlWrapperException(SerializationInfo info, StreamingContext context) : base(info, context)
        {
        }
    }
}