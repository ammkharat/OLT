-- update the names of custom field groups and custom fields to no longer have 'SELC-' prepended to them
UPDATE CustomField
SET
  [Name] = REPLACE([Name], 'SELC-', '')
WHERe
  [Name] LIKE 'SELC-%'
  
  
  ALTER TABLE CustomField
  ALTER COLUMN [NAME] VARCHAR(40) NOT NULL
  
  
  UPDATE CustomFieldGroup
SET
  [Name] = REPLACE([Name], 'SELC-', '')
WHERe
  [Name] LIKE 'SELC-%'


GO

